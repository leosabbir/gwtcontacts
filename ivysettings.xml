<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
<settings defaultResolver="online"/>
	<resolvers>
		<chain name="online">
			<url name="com.springsource.repository.bundles.release">
				<ivy pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
				<artifact pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
			</url>

			<url name="com.springsource.repository.bundles.milestone">
				<ivy pattern="http://repository.springsource.com/ivy/bundles/milestone/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
				<artifact pattern="http://repository.springsource.com/ivy/bundles/milestone/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
			</url>


			<url name="com.springsource.repository.bundles.external">
				<ivy pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
				<artifact pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
			</url>

			<ibiblio name="ibiblio" m2compatible="true" usepoms="true"/>
		</chain>
	</resolvers>
</ivysettings>